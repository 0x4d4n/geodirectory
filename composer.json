{
  "name": "GeoDirectory/geodirectory",
  "type": "project",
  "description": "GeoDirectory is the fastest growing Directory plugin for WordPress",
  "keywords": ["directory", "business-directory"],
  "homepage": "https://wpgeodirectory.com/",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/AyeCode/geodirectory.git"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/AyeCode/wp-font-awesome-settings"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "AyeCode/wp-font-awesome-settings": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8",
    "phpunit/phpunit-selenium": ">=1.2",
    "phpunit/php-invoker": "1.1.*"
  },
  "extra": {
    "installer-paths": {
      "includes/libraries/{$name}": ["AyeCode/wp-font-awesome-settings"]
    }
  }
  "minimum-stability": "dev"
}